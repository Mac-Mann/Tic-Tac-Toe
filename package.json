  "private": true,
  "description": "A masterful tic-tac-toe board with robust features.",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mac-Mann/Not-Your-Average-Tic-Tac-Toe.git"
  },
  "author": "Mac Goldman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mac/Mann/Not-Your-Average-Tic-Tac-Toe/issues"
  },
  "homepage": "https://github.com/Mac/Mann/Not-Your-Average-Tic-Tac-Toe#readme",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "~3.4.1",
    "typescript": "~3.7.2",
    "@material-ui/core": "*4.9.12",
    "@material-ui/icons": "*4.9.1",
    "react-redux": "*7.2.0",
    "redux": "*4.0.5",
    "redux-saga": "*1.1.3",
    "redux-thunk": "*2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint -c eslintrc.js 'src/modules/**/*.ts' --quiet --fix",
    "lint-ci": "eslint -c eslintrc.js 'src/modules/**/*.ts' --quiet --fix",
    "jest": "jest --coverage",
    "test:Watch": "jest --coverage --watchAll",
    "compile-scss": "node-sass sass -o ./src/css --watch",
    "compile-typescript": "tsc ./src/typescript/app.ts",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "prettify": "prettier --write './src/**/*.{js, jsx}",
    "dev-build": "npm run type-check && parcel ./src/index.html",
    "prod-build": "npm run type-check && parcel build ./src/index.html --out-dir ./docs"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "*5.5.0",
    "@testing-library/react": "*10.0.4",
    "@testing-library/user-event": "*10.1.0",
    "@types/enzyme": "*3.10.5",
    "@types/enzyme-adapter-react-16": "*1.0.6",
    "@types/jest": "*25.2.1",
    "@types/node": "*13.13.4",
    "@types/react": "*16.9.34",
    "@types/react-dom": "*16.9.7",
    "@types/react-redux": "*7.1.7",
    "@types/react-router-dom": "*5.1.5",
    "@types/react-transition-group": "*4.2.4",
    "@types/reactstrap": "*8.4.2",
    "@types/redux": "*3.6.0",
    "@types/redux-saga": "*0.10.5",
    "@types/redux-thunk": "*2.1.0",
    "@types/uuid": "*7.0.3",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "enzyme": "*3.11.0",
    "enzyme-adapter-react-16": "*1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "jest": "*25.5.4",
    "node-sass": "*4.14.0",
    "parcel-bundler": "*1.12.4",
    "prettier": "*2.0.5",
    "ts-jest": "*25.4.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
